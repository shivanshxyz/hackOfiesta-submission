{
	"manifest_version": 2,

	"name": "Sahaaya",
	"description": "An accessibility tool for better reading abilities",
	"version": "0.1.3",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	}
}
